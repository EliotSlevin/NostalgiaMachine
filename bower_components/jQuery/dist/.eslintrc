{
	"extends": "../src/.eslintrc",
	"rules": {
		// That is okay for built version
		"no-multiple-empty-lines": "off",

		// Because sizze is not compatible to jquery code style
		"lines-around-comment": "off",
		"space-in-parens": "off",
		"camelcase": "off",
		"computed-property-spacing": "off",
		"max-len": "off",
		"dot-notation": "off",
		"semi-spacing": "off",
		"brace-style": "off"
	}
}
